{
  "name": "<%- projectName %>",
  "version": "0.0.1",
  "description": "<%- projectDescription %>",
  "repository": "<%- repository %>",
  "homepage": "<%- repositoryHomepageUrl %>",
  "bugs": "<%- repositoryBugsUrl %>",
  "author": "<%- author %>",
  "keywords": [],
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "htmlhint": "^0.9.13",
    "prettier": "^1.13.6",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "dependencies": {},
<% if (license === 'UNLICENSED' || !isNpmPkg ) { %>
  "private": true,
<% } %>
  "license": "<%- license %>",
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "plugin:prettier/recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5",
    "proseWrap": "always",
    "semi": false,
    "singleQuote": true,
    "requirePragma": false,
    "insertPragma": false
  },
  "scripts": {
    "test": "echo 'Error: no tests specified' && exit 1"
  }
}
